{
  "name": "host",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "lint": "eslint .",
    "start": "react-native webpack-start",
    "start:standalone": "STANDALONE=1 react-native webpack-start --host 127.0.0.1 --port 8081 --experimental-debugger",
    "test": "jest",
    "pods": "(cd ios && bundle install && bundle exec pod install)",
    "pods:update": "(cd ios && bundle exec pod update)",
    "align-deps": "yarn -r align-deps",
    "check-deps": "yarn -r check-deps",
    "bundle:ios": "react-native webpack-bundle --platform ios --entry-file index.js --dev false",
    "bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev false"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "6.5.11",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-paper": "5.11.4",
    "react-native-reanimated": "3.6.1",
    "react-native-safe-area-context": "4.8.1",
    "react-native-screens": "3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@callstack/repack": "^3.7.0",
    "@react-native-community/cli-platform-android": "^12.3.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-native-vector-icons": "^10.0.3",
    "react-test-renderer": "18.2.0",
    "super-app-dependencies": "^0.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "5.0.4",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=18"
  }
}
